# LLM Provider Selection: "openrouter" (default) or "lmstudio"
LLM_PROVIDER=openrouter

# OpenRouter Configuration
OPENAI_API_KEY=your_openrouter_api_key_here

# LM Studio (local) Configuration
# LM_STUDIO_BASE_URL defaults to http://localhost:1234/v1 if not set
# LM Studio accepts any API key (placeholder used by default)
# LM_STUDIO_API_KEY=lm-studio
# LM_STUDIO_BASE_URL=http://localhost:1234/v1

